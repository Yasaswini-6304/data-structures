import java.util.Scanner;
public class Main{
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		int k=input.nextInt();
		int x=input.nextInt();
		int n=0;
		int[]array=new int[x];
		for(int i=0;i<x;i++){
		  array[i]=input.nextInt();
		    if(array[i]==k){
		        n=1;
		        System.out.println(i);
		        break;
		    }
		}
		if(n==0){
		    System.out.println("not found");
		}
		input.close();
	}
}
