import java.util.Scanner;
public class Main{
	public static void main(String[] args) {
		Scanner input=new Scanner(System.in);
		int k=input.nextInt();
		int x=input.nextInt();
		int count=0;
		int[]array=new int[x];
		for(int i=0;i<x;i++){
		  array[i]=input.nextInt();
		}
		for(int i=0;i<x;i++){
		    if(array[i]==k){
		        count+=1;
		       
		       
		    }
		}
		if(count>=1){
		    System.out.println(count);
		}
		else{
		     System.out.println(0);
		}
		input.close();
	}
}
